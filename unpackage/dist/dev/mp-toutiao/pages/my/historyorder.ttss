.blank {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -75%);
  transform: translate(-50%, -75%);
}
.blank .empty {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.blank .empty .empty_img {
  width: 356rpx;
  height: 340rpx;
}
.blank .empty .empty_img .empty_img image {
  width: 100%;
  height: 100%;
}
.blank .empty .empty_img ._img {
  width: 100%;
  height: 100%;
}
.blank .empty .empty_text {
  font-size: 32rpx;
  font-weight: 500;
  color: #a5a5a5;
  opacity: .7;
}
.blank .home {
  margin-top: 30rpx;
  background: #007aff;
  color: #fff;
  border-radius: 100rpx;
  font-size: 24rpx;
}

