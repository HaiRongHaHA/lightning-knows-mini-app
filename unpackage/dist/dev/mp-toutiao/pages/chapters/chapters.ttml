<view><view class="video_box"><video src="{{course.mediaUri}}" controls="{{true}}"></video></view><view class="content"><view class="title">{{course.title}}</view><view class="details"><view class="left"><view>已更课时：<label class="_span">{{total+"课时"}}</label></view></view></view><view class="list"><view class="list_title">课时列表</view><block tt:for="{{chapterList}}" tt:for-item="data" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['choosewatch',['$0',index],[[['chapterList','',index]]]]]]]}}" class="item" bindtap="__e"><view class="item_title">{{data.title}}</view><view class="item_content"><view class="item_left"><block tt:if="{{data.type=='VIDEO'}}"><view class="sk">视频</view></block><block tt:else><block tt:if="{{data.type=='VOICE'}}"><view class="sk">音频</view></block></block></view><view hidden="{{!(current===index&&isActive)}}" class="item_right">当前学习</view></view></view></block></view></view></view>